{
    "name": "pg-tenant-api",
    "version": "1.42.0",
    "description": "Serverless CRUD service exposing a REST HTTP interface",
    "author": "",
    "license": "MIT",
    "scripts": {
        "start": "sls offline start",
        "dynamodb-admin": "AWS_SECRET_ACCESS_KEY=fake AWS_ACCESS_KEY_ID=fake AWS_REGION=local DYNAMO_ENDPOINT=http://localhost:8000 dynamodb-admin",
        "migrate": "NODE_ENV=local node ./migrations --up",
        "migrate:down": "NODE_ENV=local node ./migrations --down",
        "test:e2e": "NODE_ENV=test node_modules/.bin/mocha e2e/* --recursive --exit "
    },
    "dependencies": {
        "@dhteam/pg-nodejs": "^1.0.6-beta.0",
        "aws-sdk": "^2.826.0",
        "moment": "^2.29.1",
        "uuid": "^2.0.3"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "dotenv-json": "^1.0.0",
        "mocha": "^8.2.1",
        "serverless-offline": "^6.8.0",
        "supertest": "^6.1.3"
    }
}
